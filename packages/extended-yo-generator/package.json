{
  "name": "@manuth/extended-yo-generator",
  "version": "4.3.0",
  "description": "An extended version of the Yeoman-Generator",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "yo",
    "extended"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "homepage": "https://github.com/manuth/ExtendedYoGenerator/tree/master/packages/extended-yo-generator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/ExtendedYoGenerator.git",
    "directory": "packages/extended-yo-generator"
  },
  "bugs": {
    "url": "https://github.com/manuth/ExtendedYoGenerator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "rebuild": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "clean": "rimraf ./lib",
    "lint-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint": "npm run lint-base -- --ext .js,.jsx,.ts,.tsx ./src .eslintrc.js",
    "lint-compact": "npm run lint -- -f compact || exit 0",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "@types/inquirer": "7.3.0",
    "@types/yeoman-generator": "^4.11.0",
    "chalk": "^4.1.0",
    "inquirer": "^7.3.3",
    "pkg-up": "^3.1.0",
    "yeoman-generator": "^4.11.0"
  },
  "devDependencies": {
    "@manuth/extended-yo-generator-test": "^4.3.0",
    "@types/ejs": "^3.0.4",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.26",
    "@types/pkg-up": "^3.1.0",
    "@types/yeoman-test": "^2.0.4",
    "ejs": "^3.1.3",
    "fs-extra": "^9.0.1",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "temp-filesystem": "^1.1.3",
    "typescript": "^3.9.7",
    "upath": "^1.2.0",
    "yeoman-test": "^2.7.0"
  }
}
