{
  "name": "@manuth/extended-yo-generator-test",
  "version": "6.0.0",
  "description": "Provides tools for testing generators created using `@manuth/extended-yo-generator`",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "yo",
    "extended",
    "Testing"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "homepage": "https://github.com/manuth/ExtendedYoGenerator/tree/master/packages/extended-yo-generator-test#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/ExtendedYoGenerator.git",
    "directory": "packages/extended-yo-generator-test"
  },
  "bugs": {
    "url": "https://github.com/manuth/ExtendedYoGenerator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy-files && tsc -b tsconfig.build.json",
    "copy-files": "cd ../.. && npm run copy-files",
    "rebuild": "npm run clean && npm run build",
    "watch": "concurrently --raw \"npm run copy-files -- -- --watch > nay\" \"npm run build -- --watch\"",
    "clean": "rimraf ./lib",
    "lint-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint": "npm run lint-base -- --ext .js,.jsx,.ts,.tsx ./src .eslintrc.js",
    "lint-compact": "npm run lint -- -f compact || exit 0",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "@manuth/extended-yo-generator": "^6.0.0",
    "@types/yeoman-test": "^2.0.4",
    "comment-json": "^3.0.3",
    "fs-extra": "^9.0.1",
    "random-js": "^2.1.0",
    "temp-filesystem": "^1.1.3",
    "upath": "^1.2.0",
    "yeoman-test": "^3.0.0"
  },
  "devDependencies": {
    "@types/comment-json": "^1.1.1",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "concurrently": "^5.3.0",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^8.1.1",
    "nay": "^1.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2"
  }
}
