{
  "name": "extended-yo-generator",
  "private": true,
  "scripts": {
    "build": "npm run copy-files && tsc -b tsconfig.build.json",
    "copy-files": "glob-exec --foreach \"./packages/*\" --parallel -- copy-and-watch ./{LICENSE,.npmignore} \"{{file}}\"",
    "rebuild": "npm run clean && npm run build",
    "watch": "concurrently --raw \"npm run copy-files -- --watch | dev-null\" \"npm run build -- --watch\"",
    "clean": "lerna run clean",
    "lint-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint-local": "npm run lint-base -- --ext .js,.jsx,.ts,.tsx .eslintrc.js",
    "lint-local-compact": "npm run lint-local -- -f compact || exit 0",
    "lint": "npm run lint-local && lerna run lint",
    "lint-compact": "npm run lint-local-compact && lerna run lint-compact",
    "test": "lerna run test",
    "version": "ts-node ./scripts/bumpVersion.ts",
    "prepare": "lerna bootstrap && npm run rebuild"
  },
  "dependencies": {},
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^2.1.2",
    "@manuth/tsconfig": "^1.2.2",
    "@types/git-branch": "^2.0.1",
    "@types/node": "^14.6.0",
    "@types/npm-which": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "concurrently": "^5.3.0",
    "copy-and-watch": "^0.1.5",
    "dev-null-cli": "^1.0.0",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.2.4",
    "git-branch": "^2.0.1",
    "glob-exec": "^0.1.1",
    "lerna": "^3.22.1",
    "npm-which": "^3.0.1",
    "random-js": "^2.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "typescript-eslint-plugin": "^1.2.1"
  }
}
