  },
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^2.1.1",
    "@manuth/tsconfig": "^1.2.2",
    "@manuth/tslint-presets": "^1.0.4",
    "@types/ejs": "^3.0.4",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.12",
