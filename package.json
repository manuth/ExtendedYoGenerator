{
  "name": "extended-yo-generator",
  "private": true,
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "rebuild": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "clean": "lerna run clean",
    "lint-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint-local": "npm run lint-base -- --ext .js,.jsx,.ts,.tsx .eslintrc.js",
    "lint-local-compact": "npm run lint-local -- -f compact || exit 0",
    "lint": "npm run lint-local && lerna run lint",
    "lint-compact": "npm run lint-local-compact && lerna run lint-compact",
    "test": "lerna run test",
    "version": "ts-node ./scripts/bumpVersion.ts",
    "prepare": "lerna bootstrap && npm run rebuild"
  },
  "dependencies": {},
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^2.1.2",
    "@manuth/tsconfig": "^1.2.2",
    "@types/git-branch": "^2.0.1",
    "@types/node": "^14.0.22",
    "@types/npm-which": "^3.0.0",
    "@types/random-js": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^29.1.3",
    "git-branch": "^2.0.1",
    "lerna": "^3.22.1",
    "npm-which": "^3.0.1",
    "random-js": "^2.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "typescript-eslint-plugin": "^1.2.1"
  }
}
