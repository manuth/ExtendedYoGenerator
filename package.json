    "temp-filesystem": "^1.1.3",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5",
    "typescript-eslint-plugin": "^1.2.1",
    "upath": "^1.2.0",
    "yeoman-test": "^2.6.0"
