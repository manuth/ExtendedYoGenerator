  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "yo",
    "extended"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/ExtendedYoGenerator.git"
  },
