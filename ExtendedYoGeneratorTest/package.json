{
  "name": "@manuth/extended-yo-generator-test",
  "version": "3.1.2",
  "description": "Provides tools for testing generators created using `@manuth/extended-yo-generator`",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "yo",
    "extended",
    "Testing"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "homepage": "https://github.com/manuth/ExtendedYoGenerator/tree/master/ExtendedYoGeneratorTest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/ExtendedYoGenerator.git",
    "directory": "ExtendedYoGeneratorTest"
  },
  "bugs": {
    "url": "https://github.com/manuth/ExtendedYoGenerator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p .",
    "rebuild": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "clean": "rimraf ./lib",
    "lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./src",
    "lint-compact": "npm run lint -- -f compact || exit 0",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "@manuth/extended-yo-generator": "^3.1.2",
    "@types/yeoman-test": "^2.0.4",
    "yeoman-test": "^2.6.0"
  },
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^2.1.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.20",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^29.1.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "typescript-eslint-plugin": "^1.2.1"
  }
}
